{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true
  }
}
